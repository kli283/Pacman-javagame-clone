<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuControl">
   <children>
      <ImageView fitHeight="767.0" fitWidth="1024.0" layoutX="1.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../view/Resources/titleScreen.png" />
         </image>
      </ImageView>
      <Button fx:id="playButton" layoutX="362.0" layoutY="330.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="70.0" prefWidth="300.0" text="PLAY" />
      <Button fx:id="settingsButton" layoutX="362.0" layoutY="420.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="70.0" prefWidth="300.0" text="HELP AND OPTIONS" />
      <Button fx:id="creditsButton" layoutX="362.0" layoutY="510.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="70.0" prefWidth="300.0" text="CREDITS" />
      <Button fx:id="quitButton" layoutX="362.0" layoutY="600.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="70.0" prefWidth="300.0" text="QUIT" />

   </children>
</AnchorPane>
